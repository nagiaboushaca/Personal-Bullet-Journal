      <children>
         <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="300.0">
            <children>
               <ImageView fitHeight="298.0" fitWidth="220.0" pickOnBounds="true" preserveRatio="true" translateY="20.0">
                  <image>
                     <Image url="@skull.gif" />
                  </image>
                  <effect>
                     <MotionBlur />
                  </effect>
               </ImageView>
               <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                        <children>
                           <Text fill="#434343" strokeType="OUTSIDE" strokeWidth="0.0" text="Repo Reapers" textAlignment="CENTER" translateY="10.0" underline="true" wrappingWidth="204.21630859375">
