                  </children>
               </VBox>
               <Button fx:id="continueSplash" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" opacity="0.0" prefHeight="300.0" prefWidth="300.0" />
